{
  "buildCommand": "pnpm build:web",
  "outputDirectory": "packages/client/dist",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": null,
  "name": "uwucord-web",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "BRAND_NAME": "uwucord"
  }
}
